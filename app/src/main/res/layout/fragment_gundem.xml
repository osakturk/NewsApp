                android:id="@+id/_btn_gundem17"
                android:layout_width="70dp"
                android:layout_height="70dp"

                android:clickable="true"
                android:src="@drawable/yenisafak"
                android:layout_marginTop="20dp"/>
            <ImageView
                android:id="@+id/_btn_gundem18"
                android:layout_width="70dp"
                android:layout_height="70dp"

                android:clickable="true"
                android:src="@drawable/radikal"
                android:layout_marginTop="20dp"/>
            <ImageView
                android:id="@+id/_btn_gundem19"
                android:layout_width="70dp"
                android:layout_height="70dp"

                android:clickable="true"
                android:src="@drawable/trthaber"
                android:layout_marginTop="20dp"/>
            <ImageView
                android:id="@+id/_btn_gundem20"
                android:layout_width="70dp"
                android:layout_height="70dp"

                android:clickable="true"
                android:src="@drawable/t24"
                android:layout_marginTop="20dp"/>


        </LinearLayout>
    </LinearLayout>
    <TextView
        android:id="@+id/txt_konum"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="150dp"
        android:text="Konumlar burda"/>



</RelativeLayout>
