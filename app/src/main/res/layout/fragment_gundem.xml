<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.omerakturk.gazetewithmenu.fragment.GundemFragment">

    <!-- TODO: Update blank fragment layout -->


    <LinearLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        android:gravity="center"
        android:layout_centerVertical="true"
        android:layout_centerHorizontal="true">



        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="vertical"

            >
            <ImageView
                android:id="@+id/_btn_gundem1"
                android:layout_width="70dp"
                android:layout_height="70dp"
                android:clickable="true"
                android:layout_marginTop="10dp"
                android:src="@drawable/ensonhaber"

                />
            <ImageView
                android:id="@+id/_btn_gundem2"
                android:layout_width="70dp"
                android:layout_height="70dp"
                android:layout_below="@+id/_btn_gundem1"
                android:src="@drawable/sabah"
                android:layout_marginTop="20dp"
                android:clickable="true"
                />
            <ImageView
                android:id="@+id/_btn_gundem3"
                android:layout_width="70dp"
                android:layout_height="70dp"
                android:layout_below="@+id/_btn_gundem2"
                android:src="@drawable/ntv"
                android:clickable="true"
                />
            <ImageView
                android:id="@+id/_btn_gundem4"
                android:layout_width="70dp"
                android:layout_height="70dp"
                android:layout_below="@id/_btn_gundem3"
                android:clickable="true"
                android:src="@drawable/aa"
                android:layout_marginTop="20dp"
                />
            <ImageView
                android:id="@+id/_btn_gundem5"
                android:layout_width="70dp"
                android:layout_height="70dp"
                android:layout_below="@id/_btn_gundem4"
                android:clickable="true"

                android:src="@drawable/takvim"
                android:layout_marginTop="20dp"/>

        </LinearLayout>
        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            android:layout_marginLeft="20dp">
            <ImageView
                android:id="@+id/_btn_gundem6"
                android:layout_width="70dp"
                android:layout_height="70dp"
                android:layout_below="@id/_btn_gundem4"
                android:clickable="true"
                android:src="@drawable/akit"
                android:layout_marginTop="10dp"/>
            <ImageView
                android:id="@+id/_btn_gundem7"
                android:layout_width="70dp"
                android:layout_height="70dp"
                android:layout_below="@id/_btn_gundem4"
                android:clickable="true"
                android:src="@drawable/hurriyet"
                android:layout_marginTop="20dp"/>
            <ImageView
                android:id="@+id/_btn_gundem8"
                android:layout_width="70dp"
                android:layout_height="70dp"
                android:layout_below="@id/_btn_gundem4"
                android:clickable="true"
                android:src="@drawable/sozcu"
                android:layout_marginTop="20dp"/>
            <ImageView
                android:id="@+id/_btn_gundem9"
                android:layout_width="70dp"
                android:layout_height="70dp"
                android:layout_below="@id/_btn_gundem4"
                android:clickable="true"
                android:src="@drawable/cumhuriyet"
                android:layout_marginTop="20dp"/>
            <ImageView
                android:id="@+id/_btn_gundem10"
                android:layout_width="70dp"
                android:layout_height="70dp"
                android:layout_below="@id/_btn_gundem4"
                android:clickable="true"
                android:src="@drawable/aksam"
                android:layout_marginTop="20dp"/>

        </LinearLayout>
        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            android:layout_marginLeft="20dp">
            <ImageView
                android:id="@+id/_btn_gundem11"
                android:layout_width="70dp"
                android:layout_height="70dp"

                android:clickable="true"
                android:src="@drawable/cnnturk"
                android:layout_marginTop="10dp"/>
            <ImageView
                android:id="@+id/_btn_gundem12"
                android:layout_width="70dp"
                android:layout_height="70dp"

                android:clickable="true"
                android:src="@drawable/star"
                android:layout_marginTop="20dp"/>
            <ImageView
                android:id="@+id/_btn_gundem13"
                android:layout_width="70dp"
                android:layout_height="70dp"

                android:clickable="true"
                android:src="@drawable/karar"
                android:layout_marginTop="20dp"/>
            <ImageView
                android:id="@+id/_btn_gundem14"
                android:layout_width="70dp"
                android:layout_height="70dp"

                android:clickable="true"
                android:src="@drawable/milliyet"
                android:layout_marginTop="20dp"/>
            <ImageView
                android:id="@+id/_btn_gundem15"
                android:layout_width="70dp"
                android:layout_height="70dp"

                android:clickable="true"
                android:src="@drawable/posta"
                android:layout_marginTop="20dp"/>

        </LinearLayout>
        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            android:layout_marginLeft="20dp">
            <ImageView
                android:id="@+id/_btn_gundem16"
                android:layout_width="70dp"
                android:layout_height="70dp"

                android:clickable="true"
                android:src="@drawable/haberturk"
                android:layout_marginTop="10dp"/>
